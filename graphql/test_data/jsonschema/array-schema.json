{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "ArraySchema",
    "description": "A schema with a few array fields.",
    "type": "object",
    "properties": {
        "arrayStringField": {
            "description": "Sample array field description.",
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "arrayObjectField": {
            "description": "Sample array field description.",
            "type": "array",
            "items": {
                "type": "object",
                "description": "Sample object array item field description.",
                "properties": {
                    "objectField": {
                        "type": "string"
                    }
                }
            }
        }
    } 
}